{
  "name": "sed-frontend",
  "version": "1.1.0",
  "private": false,
  "scripts": {
    "build": "fec build",
    "deploy": "npm-run-all build:prod lint test",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "npm run deploy",
    "postinstall": "ts-patch install",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "fec dev",
    "start:proxy": "PROXY=true fec dev",
    "test": "TZ=UTC jest --passWithNoTests --verbose --no-cache",
    "test:coverage": "curl -sSL 'https://raw.githubusercontent.com/RedHatInsights/insights-interact-tools/refs/heads/main/scripts/coverage.sh' | bash",
    "verify": "npm-run-all build lint test",
    "ci:verify": "npm run test:coverage"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "verbose": true,
    "testRunner": "jest-jasmine2",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "coverageDirectory": "coverage-jest",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(lodash-es|@redhat-cloud-services|@openshift|@scalprum))"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.12.13",
    "@patternfly/react-core": "^5.3.4",
    "@patternfly/react-icons": "^5.0.0",
    "@patternfly/react-table": "^5.0.0",
    "@redhat-cloud-services/config-manager-client": "^4.0.1",
    "@redhat-cloud-services/frontend-components": "^7.0.4",
    "@redhat-cloud-services/frontend-components-notifications": "^6.1.6",
    "@redhat-cloud-services/frontend-components-utilities": "^7.0.4",
    "@sentry/webpack-plugin": "^3.1.1",
    "@tanstack/react-query": "^4.29.7",
    "@unleash/proxy-client-react": "^3.3.0",
    "classnames": "2.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.9",
    "react-router-dom": "^6.6.1",
    "redux": "^4.2.0",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "^6.2.0",
    "unleash-proxy-client": "^2.2.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@patternfly/patternfly": "^4.222.4",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^3.0.0",
    "@redhat-cloud-services/frontend-components-config": "^6.6.11",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.13",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "axios-mock-adapter": "^1.19.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "=26.6.3",
    "babel-loader": "^9.2.1",
    "babel-plugin-dual-import": "1.2.1",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "bdd-lazy-var": "^2.6.1",
    "cross-fetch": "^3.0.6",
    "css-loader": "5.0.1",
    "eslint": "8.9.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^29.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-jasmine2": "^29.7.0",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "react-test-renderer": "^18.2.0",
    "stylelint": "13.9.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0",
    "ts-patch": "^3.2.1",
    "typescript": "^5.5.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "insights": {
    "appname": "connector"
  },
  "routes": {
    "connector": "/*",
    "connectSystemsModal": "/connect-systems"
  }
}
